<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.recyclerviewtabs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name="com.example.recyclerviewtabs.views.OnClickActivity" />
        <activity android:name="com.example.recyclerviewtabs.views.TabThreeActivity">
        </activity>
        <activity android:name="com.example.recyclerviewtabs.views.TabTwoActivity" />
        <activity android:name="com.example.recyclerviewtabs.views.TabOneActivity" />
        <activity android:name="com.example.recyclerviewtabs.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>